    "operation": [
      {
        "id": "fetchDocumentMedia",
        "name": "Fetch Document Media",
        "configuration": {
          "apiConfig": {
            "method": "GET",
            "url": "[[drupalDomain]]/jsonapi/media/document",
            "pagination": {
              "cursorPagination": {
                "cursor": "links.next.href",
                "cursorType": "FULL_URL",
                "detectCursorInHeader": false
              }
            },
            "headers": {
              "Accept": "application/vnd.api+json",
              "Content-Type": "application/vnd.api+json"
            },
            "queryParams": {
              "per_page": "50"
            },
            "format": "JSON"
          }
        },
        "baseSelector": {
          "jsonSelector": {
            "label": "label",
            "content": "data"
          }
        },
        "input": [
          {
            "id": "drupalDomain",
            "name": "Drupal Domain",
            "tooltip": "Input the full URL including the \"http://\"",
            "typeId": "type.string",
            "type": {
              "stringType": {
                "stereotype": "SIMPLE",
                "maxLength": 255,
                "contentFlags": {
                  "noHtmlTags": true
                }
              }
            },
            "sensitive": false
          }
        ]
