{
    "id": "wistia",
    "name": "Wistia",
    "logo": "wistia-icon.svg",
    "operation": [
        {
            "id": "fetchVideos",
            "name": "Fetch Videos",
            "productFeatureId": [540],
            "configuration": {
                "apiConfig": {
                    "method": "GET",
                    "url": "https://api.wistia.com/v1/medias.json",
                    "bearerToken": "[[bearerToken]]",
                    "pagination": {
                        "pageBasedPagination": {
                            "pageKey": "page",
                            "pageStartValue": 1,
                            "limitKey": "per_page",
                            "limitValue": 100
                        }
                    },
                    "perSecondRequestLimit": 10,
                    "headers": {},
                    "queryParams": {},
                    "dataFormat": "JSON"
                }
            },
            "baseSelector": {
                "jsonSelector": {
                    "label": "label",
                    "content": "[*]"
                }
            },
            "input": [
                {
                    "id": "bearerToken",
                    "name": "Bearer Token",
                    "tooltip": "The master token automatically generated for your Wistia account. You can find it in the API tab of your Account Settings.",
                    "typeId": "type.string",
                    "type": {
                        "stringType": {
                            "stereotype": "SIMPLE",
                            "maxLength": 100,
                            "contentFlags": {
                                "noHtmlTags": true
                            }
                        }
                    },
                    "sensitive": true
                }
            ]
        }
    ]
}